{
  "name": "@hbcode/mf-react",
  "scripts": {
    "start": "webpack serve",
    "start:standalone": "webpack serve --env standalone",
    "build": "webpack --mode=production",
    "analyze": "webpack --mode=production --env analyze",
    "lint": "eslint src --ext js,ts,tsx",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "test": "cross-env BABEL_ENV=test jest",
    "watch-tests": "cross-env BABEL_ENV=test jest --watch",
    "coverage": "cross-env BABEL_ENV=test jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && concurrently npm:test npm:lint"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/runtime": "^7.16.3",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.2.0",
    "babel-jest": "^27.5.0",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.0",
    "jest-cli": "^27.5.0",
    "prettier": "^2.5.0",
    "pretty-quick": "^4.0.0",
    "single-spa-react": "^4.0.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.0",
    "webpack-config-single-spa-react": "^3.0.0",
    "webpack-dev-server": "^4.0.0",
    "webpack-merge": "^6.0.0",
    "@babel/preset-typescript": "^7.16.0",
    "typescript": "^4.5.4",
    "webpack-config-single-spa-ts": "^3.0.0",
    "ts-config-single-spa": "^3.0.0",
    "@types/testing-library__jest-dom": "^5.15.0",
    "eslint-config-ts-react-important-stuff": "^3.0.0",
    "webpack-config-single-spa-react-ts": "^3.0.0"
  },
  "dependencies": {
    "@types/jest": "^27.0.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/systemjs": "^6.2.1",
    "@types/webpack-env": "^1.16.0",
    "react": "^17.0.3",
    "react-browser-router": "^3.0.0",
    "react-dom": "^17.0.3",
    "react-router-dom": "^6.0.2"
  }
}
